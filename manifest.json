{
    "manifest_version": 2,
    "name": "History Manager",
    "version": "0.0",

    "default_locale": "en",

    "chrome_url_overrides": {
        "newtab": "index.html"
    },

    "optional_permissions": [
        "downloads"
    ],

    "permissions": [
        "bookmarks",
        "history",
        "storage",
        "unlimitedStorage"
    ]
}